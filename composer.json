{
    "name": "tkhamez/eve-api",
    "description": "Client library for EVE Online API (ESI)",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "eve-online",
        "ESI"
    ],
    "homepage": "https://github.com/tkhamez/eve-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenAPI",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Tkhamez\\Eve\\API\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Tkhamez\\Eve\\API\\Test\\" : "test/" }
    }
}
